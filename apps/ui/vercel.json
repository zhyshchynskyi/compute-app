{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "default-src https: 'unsafe-inline' 'unsafe-eval'; connect-src https: http: ws:"
        }
      ]
    }
  ]
}
